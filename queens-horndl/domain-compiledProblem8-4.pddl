(define (domain queens)
(:requirements :adl :derived-predicates)
(:constants
  aa ab ac ad ae af ag ah ba bb bc bd be bf bg bh ca cb cc cd ce cf cg ch da db dc dd de df dg dh ea eb ec ed ee ef eg eh fa fb fc fd fe ff fg fh ga gb gc gd ge gf gg gh ha hb hc hd he hf hg hh)
(:predicates
  (queen ?x)
  (figure ?x)
  (rightdiagonal ?x ?y)
  (backdiagonal ?x ?y)
  (inline ?x ?y)
  (horinline ?x ?y)
  (verinline ?x ?y)
  (DATALOG_INLINE ?x0 ?x1)
  (DATALOG_QUERY0 ?x0 ?x1)
  (DATALOG_RIGHTDIAGONAL ?x0 ?x1)
  (DATALOG_HORINLINE ?x0 ?x1)
  (DATALOG_BACKDIAGONAL ?x0 ?x1)
  (DATALOG_VERINLINE ?x0 ?x1)
  (DATALOG_FIGURE ?x0)
  (AUX0 ?y0 ?x0 ?x1)
  (AUX1 ?y0 ?x0 ?x1)
  (AUX2 ?y0 ?x0 ?x1)
  (AUX3 ?y0 ?x0 ?x1)
  (AUX4 ?y ?x))
(:derived (DATALOG_QUERY0 ?x0 ?x1)
          (and (DATALOG_INLINE ?x0 ?x1) (queen ?x0)))
(:derived (DATALOG_INLINE ?x0 ?x1)
          (or (DATALOG_BACKDIAGONAL ?x0 ?x1) (DATALOG_HORINLINE ?x0 ?x1) (DATALOG_INLINE ?x1 ?x0) (DATALOG_RIGHTDIAGONAL ?x0 ?x1) (DATALOG_VERINLINE ?x0 ?x1) (inline ?x0 ?x1)))
(:derived (DATALOG_RIGHTDIAGONAL ?x0 ?x1)
          (or (exists (?y0) (AUX0 ?y0 ?x0 ?x1)) (DATALOG_RIGHTDIAGONAL ?x1 ?x0) (rightdiagonal ?x0 ?x1)))
(:derived (DATALOG_HORINLINE ?x0 ?x1)
          (or (exists (?y0) (AUX1 ?y0 ?x0 ?x1)) (DATALOG_HORINLINE ?x1 ?x0) (horinline ?x0 ?x1)))
(:derived (DATALOG_BACKDIAGONAL ?x0 ?x1)
          (or (exists (?y0) (AUX2 ?y0 ?x0 ?x1)) (DATALOG_BACKDIAGONAL ?x1 ?x0) (backdiagonal ?x0 ?x1)))
(:derived (DATALOG_VERINLINE ?x0 ?x1)
          (or (exists (?y0) (AUX3 ?y0 ?x0 ?x1)) (DATALOG_VERINLINE ?x1 ?x0) (verinline ?x0 ?x1)))
(:derived (DATALOG_FIGURE ?x0)
          (or (figure ?x0) (queen ?x0)))
(:derived (AUX0 ?y0 ?x0 ?x1)
          (and (DATALOG_RIGHTDIAGONAL ?x0 ?y0) (DATALOG_RIGHTDIAGONAL ?y0 ?x1)))
(:derived (AUX1 ?y0 ?x0 ?x1)
          (and (DATALOG_HORINLINE ?x0 ?y0) (DATALOG_HORINLINE ?y0 ?x1)))
(:derived (AUX2 ?y0 ?x0 ?x1)
          (and (DATALOG_BACKDIAGONAL ?x0 ?y0) (DATALOG_BACKDIAGONAL ?y0 ?x1)))
(:derived (AUX3 ?y0 ?x0 ?x1)
          (and (DATALOG_VERINLINE ?x0 ?y0) (DATALOG_VERINLINE ?y0 ?x1)))
(:derived (AUX4 ?y ?x)
          (or (= ?x ?y) (not (DATALOG_INLINE ?x ?y)) (not (queen ?x)) (not (queen ?y))))
(:action Move
  :parameters (?x ?y)
  :precondition (and (DATALOG_QUERY0 ?x ?y) (not (DATALOG_FIGURE ?y)))
  :effect (and (not (queen ?x)) (queen ?y)))
)