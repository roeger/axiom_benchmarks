(define (domain robot)
(:requirements :adl :derived-predicates)
(:constants
  robot)
(:predicates
  (columns ?x)
  (rows ?x)
  (column0 ?x)
  (column1 ?x)
  (column2 ?x)
  (rightof0 ?x)
  (rightof1 ?x)
  (rightof2 ?x)
  (leftof1 ?x)
  (leftof2 ?x)
  (leftof3 ?x)
  (row0 ?x)
  (row1 ?x)
  (row2 ?x)
  (aboveof0 ?x)
  (aboveof1 ?x)
  (aboveof2 ?x)
  (belowof1 ?x)
  (belowof2 ?x)
  (belowof3 ?x)
  (DATALOG_COLUMN2 ?x0)
  (DATALOG_COLUMNS ?x0)
  (DATALOG_ABOVEOF0 ?x0)
  (DATALOG_ROWS ?x0)
  (DATALOG_BELOWOF2 ?x0)
  (DATALOG_LEFTOF3 ?x0)
  (DATALOG_INCONSISTENT)
  (DATALOG_BELOWOF3 ?x0)
  (DATALOG_LEFTOF2 ?x0)
  (DATALOG_RIGHTOF1 ?x0)
  (DATALOG_RIGHTOF0 ?x0)
  (DATALOG_ROW1 ?x0)
  (DATALOG_ABOVEOF1 ?x0)
  (AUX0 ?x0)
  (AUX1 ?y0)
  (AUX2 ?y0)
  (AUX3 ?y0)
  (AUX4 ?y0)
  (AUX5 ?x0))
(:derived (DATALOG_COLUMN2 ?x0)
          (or (AUX0 ?x0) (column2 ?x0)))
(:derived (DATALOG_COLUMNS ?x0)
          (or (DATALOG_LEFTOF3 ?x0) (DATALOG_RIGHTOF0 ?x0) (columns ?x0)))
(:derived (DATALOG_ABOVEOF0 ?x0)
          (or (DATALOG_ABOVEOF1 ?x0) (aboveof0 ?x0)))
(:derived (DATALOG_ROWS ?x0)
          (or (DATALOG_ABOVEOF0 ?x0) (DATALOG_BELOWOF3 ?x0) (rows ?x0)))
(:derived (DATALOG_BELOWOF2 ?x0)
          (or (belowof1 ?x0) (belowof2 ?x0)))
(:derived (DATALOG_LEFTOF3 ?x0)
          (or (DATALOG_LEFTOF2 ?x0) (leftof3 ?x0)))
(:derived (DATALOG_INCONSISTENT)
          (or (exists (?y0) (AUX1 ?y0)) (exists (?y0) (AUX2 ?y0)) (exists (?y0) (AUX3 ?y0)) (exists (?y0) (AUX4 ?y0))))
(:derived (DATALOG_BELOWOF3 ?x0)
          (or (DATALOG_BELOWOF2 ?x0) (belowof3 ?x0)))
(:derived (DATALOG_LEFTOF2 ?x0)
          (or (leftof1 ?x0) (leftof2 ?x0)))
(:derived (DATALOG_RIGHTOF1 ?x0)
          (or (rightof1 ?x0) (rightof2 ?x0)))
(:derived (DATALOG_RIGHTOF0 ?x0)
          (or (DATALOG_RIGHTOF1 ?x0) (rightof0 ?x0)))
(:derived (DATALOG_ROW1 ?x0)
          (or (AUX5 ?x0) (row1 ?x0)))
(:derived (DATALOG_ABOVEOF1 ?x0)
          (or (aboveof1 ?x0) (aboveof2 ?x0)))
(:derived (AUX0 ?x0)
          (and (DATALOG_LEFTOF3 ?x0) (rightof2 ?x0)))
(:derived (AUX1 ?y0)
          (and (DATALOG_ABOVEOF1 ?y0) (belowof1 ?y0)))
(:derived (AUX2 ?y0)
          (and (DATALOG_RIGHTOF1 ?y0) (leftof1 ?y0)))
(:derived (AUX3 ?y0)
          (and (DATALOG_LEFTOF2 ?y0) (rightof2 ?y0)))
(:derived (AUX4 ?y0)
          (and (DATALOG_BELOWOF2 ?y0) (aboveof2 ?y0)))
(:derived (AUX5 ?x0)
          (and (DATALOG_ABOVEOF1 ?x0) (DATALOG_BELOWOF2 ?x0)))
(:action moveRight
  :parameters (?x)
  :precondition (and (DATALOG_COLUMNS ?x) (not (DATALOG_INCONSISTENT)))
  :effect (and (when (DATALOG_RIGHTOF0 ?x) (and (rightof1 ?x))) (when (DATALOG_RIGHTOF1 ?x) (and (rightof2 ?x))) (when (leftof1 ?x) (and (leftof2 ?x) (not (leftof1 ?x)))) (when (DATALOG_LEFTOF2 ?x) (and (leftof3 ?x) (not (leftof2 ?x))))))
(:action moveLeft
  :parameters (?x)
  :precondition (and (DATALOG_COLUMNS ?x) (not (DATALOG_INCONSISTENT)))
  :effect (and (when (DATALOG_LEFTOF2 ?x) (and (leftof1 ?x))) (when (DATALOG_LEFTOF3 ?x) (and (leftof2 ?x))) (when (DATALOG_RIGHTOF1 ?x) (and (rightof0 ?x) (rightof1 ?x))) (when (rightof2 ?x) (and (rightof1 ?x) (rightof2 ?x)))))
(:action moveUp
  :parameters (?x)
  :precondition (and (DATALOG_ROWS ?x) (not (DATALOG_INCONSISTENT)))
  :effect (and (when (DATALOG_ABOVEOF0 ?x) (and (aboveof1 ?x))) (when (DATALOG_ABOVEOF1 ?x) (and (aboveof2 ?x))) (when (belowof1 ?x) (and (belowof2 ?x) (not (belowof1 ?x)))) (when (DATALOG_BELOWOF2 ?x) (and (belowof3 ?x) (not (belowof2 ?x))))))
(:action moveDown
  :parameters (?x)
  :precondition (and (DATALOG_ROWS ?x) (not (DATALOG_INCONSISTENT)))
  :effect (and (when (DATALOG_BELOWOF2 ?x) (and (belowof1 ?x))) (when (DATALOG_BELOWOF3 ?x) (and (belowof2 ?x))) (when (DATALOG_ABOVEOF1 ?x) (and (aboveof0 ?x) (not (aboveof1 ?x)))) (when (aboveof2 ?x) (and (aboveof1 ?x) (not (aboveof2 ?x))))))
)